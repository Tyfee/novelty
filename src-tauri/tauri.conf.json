{
    "build": {
      "beforeDevCommand": "npm run dev",
      "beforeBuildCommand": "npm run build",
      "devPath": "http://localhost:1420",
      "distDir": "../build"
    },
    "package": {
      "productName": "novelty",
      "version": "0.0.0"
    },
    "tauri": {
      "bundle": {
        "identifier": "com.tyfee.novelty"
      },
      "windows": [
        {
          "title": "Novelty",
          "width": 800,
          "height": 600
        }
      ],
      "allowlist": {
        "all": false,
        "window": {
            "all": false,
            "setTitle": true
        },
        "fs": {
          "all": true
        },
        "shell": {
          "all": true
        }
      }
    }
  }
  